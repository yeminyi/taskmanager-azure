// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "dist": true,
    "dist-server": true,
    "out": true
  },
  "files.trimTrailingWhitespace": true,
  "editor.fontSize": 16,
  "editor.lineHeight": 24,
  "editor.fontLigatures": true,
  "editor.fontFamily":
    "'Fira Code', 'Operator Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.snippetSuggestions": "top",
  "editor.trimAutoWhitespace": true,
  "files.insertFinalNewline": true,
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.fontFamily":
    "'Fira Code', 'Operator Mono', Menlo, Monaco, 'Courier New', monospace",
    "appService.defaultWebAppToDeploy": "/subscriptions/96926e1f-f183-4550-ad72-b8108c622f81/resourceGroups/m1yGroup/providers/Microsoft.Web/sites/taskmanager2020",
    "appService.deploySubpath": "."
}
